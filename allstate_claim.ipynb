  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# clean up 'noisy data' in categorical variables by removing categorical values that appear only in train or test set (but not both). "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "for column in list(train.select_dtypes(include=['object']).columns):\n",
    "        if train[column].nunique() != test[column].nunique():\n",
    "            set_train = set(train[column].unique())\n",
    "            set_test = set(test[column].unique())\n",
    "            #remove ones in train but not in test\n",
    "            remove_train = set_train - set_test\n",
    "            #remove ones in test but not in train\n",
    "            remove_test = set_test - set_train\n",
    "\n",
    "            remove = remove_train.union(remove_test)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def filter_cat(x):\n",
    "                if x in remove:\n",
    "                    return np.nan\n",
    "                return x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "#concate training and test set to cover all categorical features\n",
    "train_test = pd.concat((train, test)).reset_index(drop=True)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "#apply lambda function along column\n",
    "train_test[column] = train_test[column].apply(lambda x: filter_cat(x), 1)"
   ]
  },
